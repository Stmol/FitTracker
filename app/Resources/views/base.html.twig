<!DOCTYPE html>
<html>
<head>
  <meta charset="UTF-8"/>
  <title>{% block title %}Welcome!{% endblock %}</title>
  {% block stylesheets %}
    <link href="http://fonts.googleapis.com/css?family=PT+Sans:400,700,400italic&amp;subset=latin,cyrillic"
          media="screen" rel="stylesheet" type="text/css"/>

    {% stylesheets '@stylesheets' %}
    <link rel="stylesheet" href="{{ asset_url }}"/>
    {% endstylesheets %}
  {% endblock %}
  <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}"/>
</head>
<body>
{% block body %}
  {% block menu %}
    <ul>
      <li><a href="{{ path('exercises') }}">{{ 'base.exercise_list'|trans }}</a></li>
      <li><a href="{{ path('workouts') }}">{{ 'base.workout_list'|trans }}</a></li>
    </ul>
  {% endblock menu %}

  {% block content '' %}

  {% block javascripts %}
    {#<script src="//ajax.googleapis.com/ajax/libs/jquery/2.1.0/jquery.min.js"></script>#}
    {% javascripts '@js_vendor' %}
      <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
  {% endblock javascripts %}
{% endblock %}
</body>
</html>
